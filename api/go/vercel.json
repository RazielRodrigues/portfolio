{
    "version": 2,
    "builds": [
        {
            "src": "main.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/github",
            "dest": "main.go"
        },
        {
            "src": "/devto",
            "dest": "main.go"
        }
    ]
}